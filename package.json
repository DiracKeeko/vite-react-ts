{
  "name": "vite-react-ts",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "compile": "tsc",
    "husky": "cd .. && husky install vite-react-ts/.husky",
    "lint": "eslint src --ext js,jsx,ts,tsx",
    "format": "prettier --write **/*.{js,ts,tsx} && eslint ./src --ext js,ts,tsx --fix"
  },
  "dependencies": {
    "@ant-design/pro-components": "^2.6.42",
    "@antv/g6": "^5.0.35",
    "ahooks": "^3.7.10",
    "antd": "^4.24.15",
    "axios": "^0.27.2",
    "dayjs": "^1.11.11",
    "js-financial-tools": "^2.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.x",
    "@types/react": "^18.x",
    "@types/react-dom": "^18.x",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitejs/plugin-react": "^1.3.0",
    "editorconfig": "^0.15.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "typescript": "^4.7.4",
    "vite": "^6.0.6",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-svgr": "^4.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,html}": [
      "prettier --write **/*.{js,ts,tsx}",
      "eslint ./src --ext .js,.ts,.tsx --fix"
    ]
  }
}
